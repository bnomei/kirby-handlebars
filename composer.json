{
    "name": "bnomei/kirby3-handlebars",
    "type": "kirby-plugin",
    "version": "2.1.3",
    "license": "MIT",
    "description": "Kirby 3 Component for semantic templates with Handlebars and Mustache",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "handlebars",
        "mustache",
        "cache",
        "component",
        "template-engine",
        "performance",
        "semantic",
        "logic-less"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/composer-installer": "^1.1",
        "zordius/lightncandy": "1.2.2"
    }
}
